# Environment Variables Template
# Copy this file to .env and fill in your values
# NEVER commit .env file to git (it's in .gitignore)

# iOS Configuration
IOS_BUNDLE_ID=com.geniusproject.zet
APPLE_ID=your-apple-id@example.com
APPLE_TEAM_ID=YOUR_TEAM_ID
IOS_PROVISIONING_PROFILE_NAME=your-provisioning-profile-name

# Android Configuration
ANDROID_KEYSTORE_PATH=./android/app/release.keystore
ANDROID_KEYSTORE_PASSWORD=your-keystore-password
ANDROID_KEY_ALIAS=your-key-alias
ANDROID_KEY_PASSWORD=your-key-password

# Firebase Configuration
FIREBASE_TOKEN=your-firebase-cli-token
FIREBASE_IOS_APP_ID=1:123456789:ios:abcdef
FIREBASE_ANDROID_APP_ID=1:123456789:android:abcdef

# App Store Connect API (for TestFlight)
APP_STORE_CONNECT_API_KEY_PATH=./AuthKey_XXXXXXXXXX.p8

# Google Play Console (for Play Store)
GOOGLE_PLAY_JSON_KEY_PATH=./google-play-service-account.json
